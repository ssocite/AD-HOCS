Select vt_alumni_market_sheet.id_number,
       vt_alumni_market_sheet.REPORT_NAME,
       vt_alumni_market_sheet.RECORD_STATUS_CODE,
       vt_alumni_market_sheet.FIRST_KSM_YEAR,
       vt_alumni_market_sheet.PROGRAM,
       vt_alumni_market_sheet.PROGRAM_GROUP,
       vt_alumni_market_sheet.Gender_Code,
       vt_alumni_market_sheet.fld_of_work_code,
       vt_alumni_market_sheet.fld_of_work,
       vt_alumni_market_sheet.employer_name1,
       vt_alumni_market_sheet.job_title,
       vt_alumni_market_sheet.HOUSEHOLD_CITY,
       vt_alumni_market_sheet.HOUSEHOLD_STATE,
       vt_alumni_market_sheet.HOUSEHOLD_ZIP,
       vt_alumni_market_sheet.HOUSEHOLD_GEO_CODES,
       vt_alumni_market_sheet.HOUSEHOLD_COUNTRY,
       vt_alumni_market_sheet.HOUSEHOLD_CONTINENT,
       vt_alumni_market_sheet.prospect_manager_id,
       vt_alumni_market_sheet.prospect_manager,
       vt_alumni_market_sheet.mgo_pr_score,
       vt_alumni_market_sheet.mgo_pr_model,
       vt_alumni_market_sheet.assignment_id_number,
       vt_alumni_market_sheet.Leadership_Giving_Officer
From vt_alumni_market_sheet
Where vt_alumni_market_sheet.HOUSEHOLD_GEO_CODES LIKE '%Boston%'
And vt_alumni_market_sheet.fld_of_work in (
'Management Consulting',
'Health, Wellness and Fitness',
'Hospital & Health Care',
'Biotechnology',
'Pharmaceuticals',
'Medical Devices',
'Medical Practice')
